<!doctype html>
<html lang="en">

<head>
  <%- include('../partials/head') %>
</head>

<body>
  <!-- navbar header -->
  <%- include('../partials/header') %>
  <!-- /.navbar header -->

  <!-- home content -->
  <div class="container">
    <div class="row custom-card-wrapper">
      <% for(var i = 0; i < listaProductos.productos.length; i++) { %>
      <div class="col-3">
        <a href="javascript:void(0)" class="card">
          <div class="card-img" style="background-image: url('<%=  listaProductos.productos[i].imagen %>');">
            <p class="card-title"><%=  listaProductos.productos[i].nombre %></p>
          </div>
          <div class="card-body">
          </div>
        </a>
      </div>
      <% } %>
    </div>
    <button class="btn btn-danger text-light">Entrar</button>
  </div>
  <!-- /.home content -->

  <%- include('../partials/scripts') %>
</body>

</html>
